 <script>
     function funcaoEnviar(){
    var cep = document.getElementById("ceP").value;
    var estado = document.getElementById("est").value;
    var endereço = document.getElementById("ende").value;
    var localidade = document.getElementById("local").value;
    var lab = document.getElementById("lab").value;

    document.getElementById("1").innerHTML= cep;
    document.getElementById("2").innerHTML= est;
    document.getElementById("3").innerHTML= ende;
    document.getElementById("4").innerHTML= localidade;
    document.getElementById("5").innerHTML= lab;
}
 </script>

    

